{
  "name": "fe-code-exercise",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "postinstall": "npm run seed-db",
    "seed-db": "node src/server/initJsonDb",
    "server": "json-server --watch --port 4000 --delay 1500 --routes src/server/routes.json src/server/db.json",
    "start": "concurrently \"npm run server\" \"react-scripts start\"",
    "test": "react-scripts test --watchAll=false --coverage",
    "test:watch": "react-scripts test",
    "test:lint": "eslint --ext=js --ext=jsx .",
    "test:lint:fix": "eslint --ext=js --ext=jsx --fix ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:lint",
      "pre-push": "npm run test"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:4000",
  "dependencies": {
    "axios": "^0.21.1",
    "lodash.debounce": "^4.0.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.19.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "json-server": "^0.16.3",
    "prop-types": "^15.7.2",
    "react-scripts": "4.0.1",
    "redux-mock-store": "^1.5.4"
  }
}
